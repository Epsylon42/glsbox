<!doctype html>
<html lang="en">

    <head>
        {{> head}}
    </head>

    <body>
        {{> nav}}

        <div id="content">
            <ul class="shaders">
                {{#shaders}}

                <li>
                    <div class="preview">
                        <a href="/view/{{id}}">
                            {{#previewUrl}}
                            <img src="{{.}}">
                            {{/previewUrl}}
                            {{^previewUrl}}
                            <div class="placeholder"></div>
                            {{/previewUrl}}
                        </a>
                    </div>

                    <div class="data">
                        <p>id: {{id}}</p>
                    </div>
                </li>

                {{/shaders}}
            </ul>
        </div>

        {{> footer}}
    </body>

    {{> scripts}}
</html>
